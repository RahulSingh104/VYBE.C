        "android.permission.RECORD_AUDIO",
        "android.permission.MODIFY_AUDIO_SETTINGS"
      ]
    },
    "web": {
      "favicon": "./assets/images/favicon.png"
    },
    "plugins": [
      "expo-video",
      "expo-audio"
    ],
    "extra": {
      "eas": {
        "projectId": "bfb4320d-c674-4d35-9c7f-fa2c092ceb5b"
